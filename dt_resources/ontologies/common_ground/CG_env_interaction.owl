<?xml version="1.0"?>
<rdf:RDF xmlns="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_env_interaction#"
     xml:base="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_env_interaction#"
     xmlns:gc_env_int="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_env_interaction#"
     xmlns:cg_objects="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_objects#"
     xmlns:cg_agents="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_agents#"
     xmlns:cg_entities="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_entities#"
     xmlns:cg_geometric="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_geometric#"
     xmlns:onto="ontologenius"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_env_interaction">
      <owl:imports rdf:resource="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_agents"/>
      <owl:imports rdf:resource="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_objects"/>
      <owl:imports rdf:resource="https://github.com/RIS-WITH/exp_director_task/blob/master/dt_resources/ontologies/common_ground/CG_geometric"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    


    <!-- gc_env_int#canSee -->

    <owl:ObjectProperty rdf:about="gc_env_int#canSee">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="cg_agents#Agent"/>
        <rdfs:range rdf:resource="cg_entities#PhysicalEntity"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#hasInLeftHand -->

    <owl:ObjectProperty rdf:about="gc_env_int#hasInLeftHand">
        <rdfs:subPropertyOf rdf:resource="gc_env_int#isHolding"/>
        <owl:inverseOf rdf:resource="gc_env_int#isInLeftHandOf"/>
        <rdfs:domain rdf:resource="cg_agents#Agent"/>
        <rdfs:range rdf:resource="cg_objects#Pickable"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#hasInReach -->

    <owl:ObjectProperty rdf:about="gc_env_int#hasInReach">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="gc_env_int#isReachableBy"/>
        <rdfs:domain rdf:resource="cg_agents#Agent"/>
        <rdfs:range rdf:resource="cg_objects#Object"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#hasInRightHand -->

    <owl:ObjectProperty rdf:about="gc_env_int#hasInRightHand">
        <rdfs:subPropertyOf rdf:resource="gc_env_int#isHolding"/>
        <owl:inverseOf rdf:resource="gc_env_int#isInRightHandOf"/>
        <rdfs:domain rdf:resource="cg_agents#Agent"/>
        <rdfs:range rdf:resource="cg_objects#Pickable"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isHeldBy -->

    <owl:ObjectProperty rdf:about="gc_env_int#isHeldBy">
        <rdfs:subPropertyOf rdf:resource="cg_geometric#geometricalProperty"/>
        <owl:inverseOf rdf:resource="gc_env_int#isHolding"/>
        <rdfs:domain rdf:resource="cg_objects#Pickable"/>
        <rdfs:range rdf:resource="cg_agents#Agent"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isHolding -->

    <owl:ObjectProperty rdf:about="gc_env_int#isHolding">
        <rdfs:subPropertyOf rdf:resource="cg_geometric#geometricalProperty"/>
        <rdfs:domain rdf:resource="cg_agents#Agent"/>
        <rdfs:range rdf:resource="cg_objects#Pickable"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isInLeftHandOf -->

    <owl:ObjectProperty rdf:about="gc_env_int#isInLeftHandOf">
        <rdfs:subPropertyOf rdf:resource="gc_env_int#isHeldBy"/>
        <rdfs:domain rdf:resource="cg_objects#Pickable"/>
        <rdfs:range rdf:resource="cg_agents#Agent"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isInRightHandOf -->

    <owl:ObjectProperty rdf:about="gc_env_int#isInRightHandOf">
        <rdfs:subPropertyOf rdf:resource="gc_env_int#isHeldBy"/>
        <rdfs:domain rdf:resource="cg_objects#Pickable"/>
        <rdfs:range rdf:resource="cg_agents#Agent"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isReachableBy -->

    <owl:ObjectProperty rdf:about="gc_env_int#isReachableBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="cg_objects#Object"/>
        <rdfs:range rdf:resource="cg_agents#Agent"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isSeeing -->

    <owl:ObjectProperty rdf:about="gc_env_int#isSeeing">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="gc_env_int#isSeenBy"/>
        <rdfs:domain rdf:resource="cg_agents#Agent"/>
        <rdfs:range rdf:resource="cg_objects#Object"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isSeenBy -->

    <owl:ObjectProperty rdf:about="gc_env_int#isSeenBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="cg_objects#Object"/>
        <rdfs:range rdf:resource="cg_agents#Agent"/>
    </owl:ObjectProperty>
    


    <!-- gc_env_int#isVisibleBy -->

    <owl:ObjectProperty rdf:about="gc_env_int#isVisibleBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="cg_objects#Object"/>
        <rdfs:range rdf:resource="cg_agents#Agent"/>
    </owl:ObjectProperty>
</rdf:RDF>
